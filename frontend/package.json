{
  "name": "web",
  "version": "3.0.0",
  "description": "Clean, modern frontend for BiteBase Intelligence platform",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev -p 5000",
    "dev:fast": "NODE_ENV=development SKIP_ENV_VALIDATION=1 next dev -p 5000",
    "dev:turbo": "NODE_ENV=development next dev -p 5000 --turbo",
    "build": "next build",
    "start": "next start -p 5000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.2",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.468.0",
    "negotiator": "^1.0.0",
    "next": "^15.4.4",
    "next-intl": "^4.3.6",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
